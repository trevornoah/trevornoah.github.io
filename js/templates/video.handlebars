<div class="nf-item {{filter}}">
    <div class="item-box">
        <a class="cbox-iframe" href="{{url}}" title="{{name}}">
          <img class="item-container" src="{{thumbnail}}" alt="1" />
          <div class="item-mask">
              <div class="item-caption">
                  <h5 class="white">{{name}}</h5>
              </div>
          </div>
        </a>
    </div>
</div>
